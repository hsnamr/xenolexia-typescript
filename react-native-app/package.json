{
  "name": "xenolexia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "webpack serve --mode development",
    "web:build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" \"*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint:check && npm run format:check",
    "clean": "cd android && ./gradlew clean && cd .. && cd ios && xcodebuild clean && cd ..",
    "reset": "rm -rf node_modules && rm -rf ios/Pods && npm install && cd ios && pod install",
    "prepare": "husky install"
  },
  "dependencies": {
    "xenolexia-typescript": "file:../ts-shared-core",
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@lingo-reader/mobi-parser": "^0.4.5",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "fast-xml-parser": "^4.5.0",
    "react-native-screen-brightness": "^2.0.0-alpha",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "@tanstack/react-query": "^5.51.1",
    "date-fns": "^3.6.0",
    "epubjs": "^0.3.93",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.74.3",
    "react-native-document-picker": "^9.3.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.3.0",
    "react-native-web": "^0.19.12",
    "react-native-webview": "^13.10.5",
    "uuid": "^10.0.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.24.7",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.79",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.21.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "html-webpack-plugin": "^5.6.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.3.2",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.4.5",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
